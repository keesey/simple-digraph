{
  "author": {
    "email": "keesey+phylopic@gmail.com",
    "name": "T. Michael Keesey"
  },
  "description": "Simple utilities for processing directed graphs with numerical vertices.",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "chai": "^4.3.4",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "yarn": "^1.22.17"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "graph",
    "digraph",
    "graph theory",
    "directed graph",
    "arc"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "simple-digraph",
  "repository": "https://github.com/keesey/simple-digraph",
  "resolutions": {
    "@types/node": "*"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ext .js,.ts ./src",
    "postversion": "git push && git push --tags",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "test": "mocha -r ts-node/register \"./src/**/*.spec.ts\"",
    "version": "yarn format && yarn lint --fix && git add -A src"
  },
  "types": "dist/index.d.ts",
  "version": "1.0.0"
}
